<div id='price-ticker'>
		<div class='container'>
		<div class='row'>
		<div class='col-sm-1' style='background-color:#c2e3ff;text-align:center;'>
			<span>BSE:</span>
		</div>
		<div class='col-sm-11'>
			<marquee class='BSE'>

			<%for(var i=0;i<data.item.length;i++) { %>
				<% if(data.item[i].direction==='-1'){%>	

				<span style='color:red'> <%= data.item[i].shortname%> : <%= data.item[i].lastvalue %>                    
					(<%=data.item[i].precentchange%>%)   <i class="fas fa-arrow-down"></i> &nbsp;
				</span>			 

					<%}%>
				<%if(data.item[i].direction==='1') { %>
					<span style='color:blue'>  <%= data.item[i].shortname %> : <%= data.item[i].lastvalue %>                 
					(<%=data.item[i].precentchange%>%)<i class="fas fa-arrow-up"></i>    </span> &nbsp;
				<% } %>
			<% } %>
			</marquee>
		</div>
		</div>
	</div>

	<div class='container'>
		<div class='row'>
